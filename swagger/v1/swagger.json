{
  "swagger": "2.0",
  "info": {
    "title": "API V1",
    "version": "v1"
  },
  "paths": {
    "/api/v1/rates/find": {
      "post": {
        "summary": "Finds a rate",
        "tags": [
          "Rates"
        ],
        "description": "Searches rates based on start time and end time",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "start_time": {
                  "type": "string"
                },
                "end_time": {
                  "type": "string"
                }
              },
              "required": [
                "start_time",
                "end_time"
              ]
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "examples": {
              "application/json": {
                "result": 1750,
                "msg": null
              }
            }
          },
          "422": {
            "description": "invalid user input",
            "examples": {
              "application/json": {
                "result": null,
                "msg": "Start time after end time"
              }
            }
          }
        }
      }
    }
  }
}